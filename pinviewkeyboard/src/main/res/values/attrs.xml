<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomButton">
        <attr name="ButtonType" format="enum">
            <enum name="primary" value="0"/>
            <enum name="secondary" value="1"/>
            <enum name="cancel" value="2"/>
        </attr>
        <attr name="Color" format="enum">
            <enum name="red" value="1"/>
            <enum name="blue" value="0"/>
        </attr>
        <attr name="customFontSize" format="integer"/>
    </declare-styleable>

    <declare-styleable name="PinView">
        <attr name="pinCount" format="integer"/>
    </declare-styleable>

</resources>

